<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.model.dao.GugunAddressDAO">
	
	<select id="getGugunAddressByGugunCode" parameterType="String" resultType="GugunAddress">
		select * from gugunaddress where gugunCode=#{gugunCode}
	</select>
	
	<select id="getGugunAddressAll" resultType="GugunAddress">
		select * from gugunaddress;
	</select>
	
</mapper>














