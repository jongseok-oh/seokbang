<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.model.dao.BaseAddressDAO">
	
	<select id="getBaseAddressByDongCode" parameterType="String" resultType="BaseAddress">
		select dongCode, sidoName, gugunName, dongName, lat, lng from baseaddress where dongCode=#{dongCode}
	</select>
	
</mapper>














